                                                                                if (this.checked) {
                                                                                    var otherid = $(".ms-RadioText[title='.com Chatbot'] :checkbox")[0].id;
                                                                                    $("input[id^='" + otherid + "']").prop("checked", (this.checked) ? "checked" : "");
                                                                                }
