                                                                            $(".ms-RadioText[title='Local Only'] :checkbox").click(function() {
                                                                                var otherids = (this.id).substring(0, (this.id).length - 2);
                                                                                $("input[id^='" + otherids + "']").prop("checked", (this.checked) ? "" : "");
                                                                                $("input[id^='" + this.id + "']").prop("checked", "checked");
                                                                            });

