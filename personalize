                                newCountP1++;
                            }
                            else if (priority == 2){
                                if(promoEndDate.length == 0)
                                    newStringBuilderP2 = newStringBuilderP2 + "<li class='dc-modal dc-li-modal'>" + item.title + " (P" + priority + ")" + "  " + reformatDate(new Date(launchDate)) + "</li>";
